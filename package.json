{
  "name": "caloriecalc",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www.js",
    "postinstall": "bower install ; rm -rf ./public/bower_components ; mv -fu ./bower_components ./public",
    "jsx-watch": "jsx --watch ./public/components/src ./public/components/build",
    "jsx-build": "jsx ./public/components/src ./public/components/build",
    "bfy-build": "browserify ./public/components/build/App.react.js > ./public/main.js",
    "all-build": "npm run jsx-build & npm run bfy-build",
	
    "monitor": "nodemon ./bin/www.js",
    "deploy": "gcloud preview app deploy app.yaml"
  },
  "dependencies": {
    "bower": "*",
    "body-parser": "~1.12.0",
    "cookie-parser": "~1.3.4",
    "debug": "~2.1.1",
    "express": "~4.12.2",
    "jade": "~1.9.2",
    "less-middleware": "1.0.x",
    "morgan": "~1.5.1",
    "serve-favicon": "~2.2.0",
    "jade": "*",
    "less": "*",
    "socket.io": "~1.3.4",
    "nconf": "*",
    "intel": "*",
    "mongoose": "4.0.1",
    "async": "*",
    "node-minify": "*",
    "mongoose-redis-cache": "*",
    "compression": "*",
    "express-session": "*",
    "socket.io-express-session": "*"
  },
  "devDependencies":{
    "react-tools": "latest",
    "browserify": "latest"
  }
}