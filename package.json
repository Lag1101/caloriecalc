{
  "name": "caloriecalc",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www.js",
    "build-client-debug": "browserify clientApp/index.js --debug -o public/index.js",
    "build-client": "browserify clientApp/index.js | uglifyjs --compress --mangle -o public/index.js",
    "build-3rdparty": "uglifyjs public/bower_components/jquery/dist/jquery.js public/bower_components/socket.io-client/socket.io.js public/bower_components/async/lib/async.js public/bower_components/bootstrap/dist/js/bootstrap.js public/bower_components/bootstrap-dialog/dist/js/bootstrap-dialog.js --compress --mangle -o public/3rdparty.js",
    "postinstall": "cd ./public; bower install"
  },
  "dependencies": {
    "bower": "*",
    "body-parser": "~1.12.0",
    "cookie-parser": "~1.3.4",
    "debug": "~2.1.1",
    "express": "~4.12.2",
    "jade": "~1.9.2",
    "less-middleware": "1.0.x",
    "morgan": "~1.5.1",
    "serve-favicon": "~2.2.0",
    "jade": "*",
    "less": "*",
    "socket.io": "~1.3.4",
    "nconf": "*",
    "intel": "*",
    "mongoose": "4.0.1",
    "async": "*",
    "node-minify": "*",
    "mongoose-redis-cache": "*",
    "compression": "*",
    "express-session": "*",
    "socket.io-express-session": "*",
    "connect-mongo": "*"
  },
  "devDependencies": {
    "browserify": "~10.2.4",
    "uglify-js": "~2.4.23",
    "source-map": "~0.4.2",
    "yargs": "~3.10.0",
    "uglify-to-browserify": "~1.0.2"
  }
}