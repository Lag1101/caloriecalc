{
  "name": "caloriecalc",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www.js",
    "postinstall": "bower install ; rm -rf ./public/bower_components ; cp -R ./bower_components ./public; npm run all-build",
    "build-dev": "browserify -e ./public/js/app.js -t reactify -o ./public/bundle.dev.js -d",
    "build-min": "browserify -e ./public/js/app.js -t reactify -t uglifyify -o ./public/bundle.min.js",
    "3rd-build": "uglifyjs ./public/bower_components/pubsub-js/src/pubsub.js ./public/bower_components/react/react.min.js ./public/bower_components/react-bootstrap/react-bootstrap.min.js  ./public/bower_components/jquery/dist/jquery.min.js ./public/bower_components/socket.io-client/socket.io.js ./public/bower_components/async/lib/async.js ./public/bower_components/bootstrap/dist/js/bootstrap.min.js ./public/bower_components/bootstrap-dialog/dist/js/bootstrap-dialog.min.js ./public/bower_components/mathjs/dist/math.min.js -o ./public/3rd.js",
    "all-build": "npm run build-min | npm run build-dev | npm run 3rd-build | npm run build-norm",
    "monitor": "nodemon ./bin/www.js",
    "build-norm": "browserify -e ./public/js/norm.js -t reactify -t uglifyify -o ./public/norm.min.js",
    "deploy": "gcloud preview app deploy app.yaml --set-default"
  },
  "dependencies": {
    "bower": "*",
    "body-parser": "~1.12.0",
    "cookie-parser": "~1.3.4",
    "debug": "~2.1.1",
    "express": "~4.12.2",
    "jade": "*",
    "less-middleware": "1.0.x",
    "morgan": "~1.5.1",
    "serve-favicon": "~2.2.0",
    "less": "*",
    "socket.io": "~1.3.4",
    "nconf": "*",
    "intel": "*",
    "mongoose": "latest",
    "async": "*",
    "compression": "*",
    "express-session": "*",
    "socket.io-express-session": "*",
    "connect-mongo": "latest",
    "react": "0.13.3"
  },
  "devDependencies": {
    "browserify": "^11.0.1",
    "react-tools": "0.12",
    "reactify": "^1.1.1",
    "uglifyify": "^3.0.1",
    "uglifyjs": "latest"
  }
}
