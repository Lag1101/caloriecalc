{
  "name": "caloriecalc",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www.js",
    "postinstall": "bower install ; rm -rf ./public/bower_components ; cp -R ./bower_components ./public",
    "jsx-watch": "jsx --watch ./public/components/src ./public/components/build",
    "jsx-build": "jsx ./public/components/src ./public/components/build",
    "bfy-build": "browserify  ./public/js/app.js --outfile ./public/main.js",
    "3rd-build": "uglifyjs  ./public/bower_components/jquery/dist/jquery.min.js ./public/js/socket.io.zip.js ./public/bower_components/async/lib/async.js ./public/bower_components/bootstrap/dist/js/bootstrap.min.js ./public/bower_components/bootstrap-dialog/dist/js/bootstrap-dialog.min.js ./public/bower_components/react/react.min.js ./public/bower_components/mathjs/dist/math.min.js -o ./public/3rd.js",
    "all-build": "npm run jsx-build & npm run bfy-build | npm run 3rd-build",

    "monitor": "nodemon ./bin/www.js",
    "deploy": "gcloud preview app deploy app.yaml"
  },
  "dependencies": {
    "bower": "*",
    "body-parser": "~1.12.0",
    "cookie-parser": "~1.3.4",
    "debug": "~2.1.1",
    "express": "~4.12.2",
    "jade": "~1.9.2",
    "less-middleware": "1.0.x",
    "morgan": "~1.5.1",
    "serve-favicon": "~2.2.0",
    "jade": "*",
    "less": "*",
    "socket.io": "~1.3.4",
    "nconf": "*",
    "intel": "*",
    "mongoose": "4.0.1",
    "async": "*",
    "mongoose-redis-cache": "*",
    "compression": "*",
    "express-session": "*",
    "socket.io-express-session": "*",
    "connect-mongo": "latest"
  },
  "devDependencies":{
    "react-tools": "latest",
    "browserify": "latest",
    "uglifyjs": "latest"
  }
}

